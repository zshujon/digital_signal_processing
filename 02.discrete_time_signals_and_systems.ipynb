     "cell_type": "code",
     "collapsed": false,
     "input": [
      "def imseq(n1, n2, n0):\n",
      "    n = np.array(range(n1, n2+1))\n",
      "    x = ((n - n0) == 0)\n",
      "    return n, x\n",
      "\n",
      "a, b = imseq(-9, 4, 0)\n",
      "p = plt.stem(a,b)"
     ],
